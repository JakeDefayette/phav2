{
  "name": "pha-v2",
  "version": "1.0.0",
  "description": "Personal Health Assistant version 2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "code-quality": "npm run lint && npm run format:check && npm run type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:db": "jest --testPathPattern=tests/database --maxWorkers=1",
    "test:services": "jest --testPathPattern=tests/services",
    "test:survey-mapper": "npx tsx scripts/test-survey-mapper.ts",
    "prepare": "husky",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "bundle-size": "node scripts/bundle-size-check.js",
    "bundle-size:check": "npm run build && npm run bundle-size",
    "bundle-size:analyze": "npm run build && npm run bundle-size -- --analyze",
    "bundle-size:ci": "npm run build && npm run bundle-size -- --fail",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "keywords": [
    "health",
    "assistant",
    "nextjs",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-email/components": "^0.0.41",
    "@react-email/render": "^1.1.2",
    "@react-pdf/renderer": "^4.3.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.77.2",
    "@vercel/blob": "^1.1.1",
    "chart.js": "^4.4.9",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "node-cron": "^3.0.3",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "resend": "^4.5.1",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0",
    "yup": "^1.6.1",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.2",
    "@tailwindcss/postcss": "^4.1.7",
    "@tanstack/react-query-devtools": "^5.77.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.5.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^26.1.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
